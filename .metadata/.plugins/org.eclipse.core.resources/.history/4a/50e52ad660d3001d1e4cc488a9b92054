	/**
 * @generated
 */
	protected boolean provides(org.eclipse.gmf.runtime.diagram.core.services.view.CreateNodeViewOperation op) {
		if (op.getContainerView() == null) {
			return false;
		}
		org.eclipse.gmf.runtime.emf.type.core.IElementType elementType = getSemanticElementType(op.getSemanticAdapter());
		org.eclipse.emf.ecore.EObject domainElement = getSemanticElement(op.getSemanticAdapter());
		int visualID;
		if (op.getSemanticHint() == null) {
			// Semantic hint is not specified. Can be a result of call from CanonicalEditPolicy.
			// In this situation there should be NO elementType, visualID will be determined
			// by VisualIDRegistry.getNodeVisualID() for domainElement.
			if (elementType != null || domainElement == null) {
				return false;
			}
			visualID = grdsl.diagram.part.GrdslVisualIDRegistry.getNodeVisualID(op.getContainerView(), domainElement);
		} else {
			visualID = grdsl.diagram.part.GrdslVisualIDRegistry.getVisualID(op.getSemanticHint());
			if (elementType != null) {
				if (!grdsl.diagram.providers.GrdslElementTypes.isKnownElementType(elementType) || (!(elementType instanceof org.eclipse.gmf.runtime.emf.type.core.IHintedType))) {
					return false; // foreign element type
				}
				String elementTypeHint = ((org.eclipse.gmf.runtime.emf.type.core.IHintedType) elementType).getSemanticHint();
				if (!op.getSemanticHint().equals(elementTypeHint)) {
					return false; // if semantic hint is specified it should be the same as in element type
				}
				if (domainElement != null && visualID != grdsl.diagram.part.GrdslVisualIDRegistry.getNodeVisualID(op.getContainerView(), domainElement)) {
					return false; // visual id for node EClass should match visual id from element type
				}
			} else {
				if (!grdsl.diagram.edit.parts.CourseSelectionEditPart.MODEL_ID.equals(grdsl.diagram.part.GrdslVisualIDRegistry.getModelID(op.getContainerView()))) {
					return false; // foreign diagram
				}
				switch (visualID) {
						case grdsl.diagram.edit.parts.StudentEditPart.VISUAL_ID:case grdsl.diagram.edit.parts.DegreeProgramEditPart.VISUAL_ID:case grdsl.diagram.edit.parts.CourseEditPart.VISUAL_ID:case grdsl.diagram.edit.parts.DepartmentEditPart.VISUAL_ID:case grdsl.diagram.edit.parts.GraduationRequirement2EditPart.VISUAL_ID:case grdsl.diagram.edit.parts.Teacher2EditPart.VISUAL_ID:				case grdsl.diagram.edit.parts.TeacherEditPart.VISUAL_ID:case grdsl.diagram.edit.parts.GraduationRequirementEditPart.VISUAL_ID:case grdsl.diagram.edit.parts.Course2EditPart.VISUAL_ID:					if (domainElement == null || visualID != grdsl.diagram.part.GrdslVisualIDRegistry.getNodeVisualID(op.getContainerView(), domainElement)) {
						return false; // visual id in semantic hint should match visual id for domain element
					}
					break;
					default:
					return false;
				}
			}
		}
		return grdsl.diagram.edit.parts.StudentEditPart.VISUAL_ID == visualID||grdsl.diagram.edit.parts.DegreeProgramEditPart.VISUAL_ID == visualID||grdsl.diagram.edit.parts.CourseEditPart.VISUAL_ID == visualID||grdsl.diagram.edit.parts.DepartmentEditPart.VISUAL_ID == visualID||grdsl.diagram.edit.parts.TeacherEditPart.VISUAL_ID == visualID||grdsl.diagram.edit.parts.GraduationRequirementEditPart.VISUAL_ID == visualID||grdsl.diagram.edit.parts.GraduationRequirement2EditPart.VISUAL_ID == visualID||grdsl.diagram.edit.parts.Course2EditPart.VISUAL_ID == visualID||grdsl.diagram.edit.parts.Teacher2EditPart.VISUAL_ID == visualID;
